{
  "name": "voby",
  "description": "A wannabe high-performance framework for writing web apps.",
  "version": "0.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tscpaths -p tsconfig.json -s src -o dist",
    "bundle": "esbuild --bundle --format=esm ./src/index.ts",
    "bench": "rm -rf node_modules/.vite && vite --config vite.js benchmark --force",
    "play": "rm -rf node_modules/.vite && vite --config vite.js playground --force",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/voby/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/voby.git"
  },
  "keywords": [
    "ui",
    "framework",
    "reactive",
    "observable",
    "fast",
    "performant",
    "small",
    "fine-grained"
  ],
  "dependencies": {
    "goober": "^2.1.8",
    "oby": "^2.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.14.23",
    "rimraf": "^3.0.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.5",
    "vite": "^2.8.4"
  }
}
